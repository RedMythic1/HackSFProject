{
  "name": "typescript-webpage",
  "version": "1.0.0",
  "description": "TypeScript frontend for Tech Deep Dive",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "npm run dev",
    "start:frontend": "webpack serve --mode development",
    "start:backend": "node api/server.js",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "webpack --mode production",
    "vercel-build": "npm install && npm run build"
  },
  "dependencies": {
    "@vercel/blob": "^0.14.0",
    "@vercel/edge-config": "^1.4.0",
    "@xenova/transformers": "^2.8.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^10.3.3",
    "jsdom": "^22.1.0",
    "terser-webpack-plugin": "^5.3.10"
  },
  "devDependencies": {
    "crypto-browserify": "^3.12.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "path-browserify": "^1.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "concurrently": "^8.2.2"
  }
}
