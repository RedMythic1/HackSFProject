{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es2015",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "lib": ["es2015", "dom"]
  },
  "include": ["src"]
} 