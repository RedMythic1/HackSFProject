{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/vercel.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/env-check.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/backtest.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/backtest.py",
      "use": "@vercel/python"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/env-check",
      "dest": "/api/env-check.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    },
    {
      "src": "/api/health",
      "dest": "/api/env-check.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    },
    {
      "src": "/api/blob-status",
      "dest": "/api/index.js",
      "continue": true
    },
    {
      "src": "/api/blob-test",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/api/list-blobs",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/dist/static/$1"
    },
    {
      "src": "/bundle.js",
      "dest": "/dist/bundle.js"
    },
    {
      "src": "/article/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/dist/index.html"
    },
    {
      "src": "/api/backtest",
      "dest": "/api/backtest.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1",
    "DEBUG": "true",
    "BLOB_READ_WRITE_TOKEN": "vercel_blob_rw_MzCMzRmJaiRlp3km_L5RVXS9InB9rTT1Aov2ZI4kzQFoT5S",
    "BLOB_URL": "https://mzcmzrmjairlp3km.public.blob.vercel-storage.com"
  }
}