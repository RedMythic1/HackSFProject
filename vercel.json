{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "tw/dist",
  "builds": [
    {
      "src": "tw/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "tw/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "tw/api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/tw/dist/static/$1"
    },
    {
      "src": "/bundle.js",
      "dest": "/tw/dist/bundle.js"
    },
    {
      "src": "/article/(.*)",
      "dest": "/tw/dist/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/tw/dist/index.html"
    }
  ]
} 