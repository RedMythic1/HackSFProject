{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@2.15.10",
      "excludeFiles": "{.next,*.cache,node_modules,public,static,stockbt,src,models,tools,__pycache__,user_data}/**"
    },
    "api/vercel.js": {
      "runtime": "@vercel/node@2.15.10",
      "excludeFiles": "{.next,*.cache,node_modules,public,static,stockbt,src,models,tools,__pycache__,user_data}/**"
    },
    "api/env-check.js": {
      "runtime": "@vercel/node@2.15.10",
      "excludeFiles": "{.next,*.cache,node_modules,public,static,stockbt,src,models,tools,__pycache__,user_data}/**"
    },
    "api/**/*.py": {
      "runtime": "@vercel/python@3.1.53",
      "excludeFiles": "{.next,*.cache,node_modules,public,static,stockbt,src,models,tools,__pycache__,user_data}/**",
      "includeFiles": "api/python_packages/**"
    }
  },
  "routes": [
    {
      "src": "/api/env-check",
      "dest": "/api/env-check.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    },
    {
      "src": "/api/health",
      "dest": "/api/env-check.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    },
    {
      "src": "/api/blob-status",
      "dest": "/api/index.js",
      "continue": true
    },
    {
      "src": "/api/blob-test",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/api/list-blobs",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/api/backtest",
      "dest": "/api/vercel.js",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/dist/static/$1"
    },
    {
      "src": "/bundle.js",
      "dest": "/dist/bundle.js"
    },
    {
      "src": "/backtesting.html",
      "dest": "/dist/backtesting.html"
    },
    {
      "src": "/backtesting.bundle.js",
      "dest": "/dist/backtesting.bundle.js"
    },
    {
      "src": "/backtesting",
      "dest": "/dist/backtesting.html"
    },
    {
      "src": "/article/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1",
    "DEBUG": "true",
    "BLOB_READ_WRITE_TOKEN": "vercel_blob_rw_MzCMzRmJaiRlp3km_L5RVXS9InB9rTT1Aov2ZI4kzQFoT5S",
    "BLOB_URL": "https://mzcmzrmjairlp3km.public.blob.vercel-storage.com",
    "PYTHON_VERSION": "3.9",
    "PYTHONPATH": "/var/task/api/python_packages:/var/task",
    "PIP_NO_DEPS": "1",
    "PIP_NO_INSTALL": "1",
    "PATH": "/var/task/bin:/var/task:/usr/local/bin:/usr/bin:/bin"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9",
      "PYTHONPATH": "/var/task/api/python_packages:/var/task",
      "PIP_NO_DEPS": "1",
      "PIP_NO_INSTALL": "1",
      "PATH": "/var/task/bin:/var/task:/usr/local/bin:/usr/bin:/bin"
    }
  },
  "installCommand": "echo 'Skipping pip installation'",
  "buildCommand": "bash -c './build.sh'"
} 