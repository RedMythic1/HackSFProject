{
  "version": 2,
  "builds": [
    {
      "src": "tw/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "tw/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/tw/api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/tw/dist/static/$1"
    },
    {
      "src": "/bundle.js",
      "dest": "/tw/dist/bundle.js"
    },
    {
      "src": "/(.*)",
      "dest": "/tw/dist/$1"
    }
  ]
} 